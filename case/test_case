#  字段格式
# 多个返回可能的时候使用&连接可能性，使用@@@进行字段胡洛
# 返回内容如果是个多级字段则为前置条件条件各级用&连接
  data_format : {
    '{"data&type":"sticker"}':{'data': {'type': 'Str', 'magics': '$$$', 'stickers': [{'key': 'Str', 'tags': ['Str'], 'scale_type': 'Int', 'name': '$$$', 'mark': 'Str', 'image': {'height': 'Int', 'width': 'Int', 'url': 'HTTP'}, 'mp4': '$$$', 'popup_duration': 'Int', 'source_text': 'Str&$$$'}], 'popup_duration': 'Int', 'extra': 'Str'}, 'errorMsg': 'Str', 'errorCode': 'Int'},
    '{"data&type":"magic"}':{'data': {'type': 'Str', 'magics': [{'key': 'Str', 'style': 'Str'}], 'stickers': '$$$', 'popup_duration': 'Int', 'extra': 'Str'}, 'errorMsg': 'Str', 'errorCode': 'Int'}
